<div id="user">
    <div class="container">
        <h1>User</h1>
        <table class="table table-bordered">
            <tbody>
            <tr>
                <th>Name</th><td><%= user.fullName %></td>
            </tr>
            <tr>
                <th>Email</th><td><%= user.emailAddress %></td>
            </tr>        
            <tr>
                <th>Cash</th><td><%= user.cash %></td>
            </tr>        
            </tbody>
        </table>
    </div>
</div>